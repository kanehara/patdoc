{
  "name": "patdoc-server",
  "version": "1.0.0",
  "description": "Node server for pacdoc",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev nodemon index.js",
    "start:production": "npm run build && NODE_ENV=production node lib/index.js",
    "db:create": "docker run -itd -p 27017:27017 -v /tmp/mongodb:/data/db --name patdocdb -d mongo && npm run db:init",
    "db:init": "node src/db-script/init",
    "db:start": "docker start patdocdb",
    "db:stop": "docker stop patdocdb",
    "build": "babel src -d lib",
    "clean": "rimraf lib",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register 'test/**/*.spec.js'"
  },
  "author": "Yohei Kanehara (kanehara@umich.edu)",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "jsonwebtoken": "^7.4.1",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.10.6",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.25.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.0.2",
    "chai-datetime": "^1.4.1",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "mocha": "^3.4.2",
    "mockgoose": "^7.3.3",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015-node",
      "stage-2"
    ]
  }
}
